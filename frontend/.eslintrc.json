{
  "env": {
    "browser": true,
    "es2020": true,
    "jest": true
  },
  "globals": {
    "shallow": true
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": "warn",
    "no-empty": "warn",
    "no-irregular-whitespace": "warn"
  }
}
